footer: function(currentPage, pageCount) {
        return [{
                style: 'tableExample',
                table: {
                    heights: [1],
                    widths: ['*'],
                    body: [
                        [{
                            border: [false, true, false, false],
                            fillColor: '#eeeeee',
                            text: ''
                        }, ],
                    ]
                }
            },
            {
                text: '第 ' + currentPage.toString() + ' 页 共 ' + pageCount + ' 页',
                alignment: 'center',
            },
        ];
    },
